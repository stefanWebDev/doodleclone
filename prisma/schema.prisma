// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model DataSetName {
  id       String @id @unique
  date     Date[]
  nameIsId String @unique
}

model Date {
  id          String      @id @unique
  dayField    DayField[]
  dataSetName DataSetName @relation(fields: [dataSetId], references: [id])
  dateIsId    String
  dataSetId   String
}

model DayField {
  id         Int     @id @default(autoincrement())
  date       Date    @relation(fields: [dateId], references: [id])
  dateId     String
  name       String
  isSelected Boolean
}
